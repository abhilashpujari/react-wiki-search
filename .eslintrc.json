{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "linebreak-style": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "react/prop-types": 0,
        "no-underscore-dangle": 0,
        "import/imports-first": [
            "error",
            "absolute-first"
        ]
    },
    "globals": {
        "window": true,
        "document": true,
        "localStorage": true,
        "FormData": true,
        "FileReader": true,
        "Blob": true,
        "navigator": true
    }
}